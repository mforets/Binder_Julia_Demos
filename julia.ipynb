   "source": [
    "using LazySets, Plots\n",
    "\n",
    "A = 1/sqrt(2.) * [1 -1; 1 1]\n",
    "Bn = n -> BallInf(ones(n), 0.2)\n",
    "\n",
    "X = Bn(2)\n",
    "Y = CH(X, expm(A) * X)"
   ]
